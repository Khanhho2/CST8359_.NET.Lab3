@model Lab3.Models.Person
@{
    Layout = "_Layout";
    ViewData["title"] = "Display Person";
}

<table>
    <tr>
        <td>First Name: </td>
        <td>@Model.FirstName</td>
    </tr>
    <tr>
        <td>Last Name: </td>
        <td>@Model.LastName</td>
    </tr>
    <tr>
        <td>Age: </td>
        <td>@Model.Age</td>
    </tr>
    <tr>
        <td>Email Address:</td>
        <td>@Model.EmailAddress</td>
    </tr>
    <tr>
        <td>Date of Birth: </td>
        <td>@Model.Birthday</td>
    </tr>
    <tr>
        <td>Description: </td>
        <td>@Model.Description</td>
    </tr>
</table>